{
  "name": "start",
  "description": "[Git flow]: Start new branch",
  "steps": {
    "start": {
      "implementation-check" : false,
      "develop": {
        "requires" : {
          "pisco-gitflow-start": "^0.0.1"
        }
      },
      "master": {
        "requires" : {
          "pisco-gitflow-start": "^0.0.1"
        }
      }
    },
    "setVersion": {
      "excludes" : ["develop", "master"],
      "component": {
        "requires" : {
          "pisco-cells-json-setVersion": "^0.0.1"
        }
      },
      "recipe": {
        "requires" : {
          "pisco-cells-json-setVersion": "^0.0.1"
        }
      },
      "app": {
        "requires" : {
          "pisco-cells-json-setVersion": "^0.0.1"
        }
      }
    },
    "push" : {
      "implementation-check" : false,
      "inputs" : {
        "branchName" : {"start": "branchName"}
      },
      "hotfix" : {
        "requires" : {
          "pisco-gitflow-publish": "^0.0.1"
        }
      },
      "feature" : {
        "requires" : {
          "pisco-gitflow-publish": "^0.0.1"
        }
      },
      "release" : {
        "requires" : {
          "pisco-gitflow-publish": "^0.0.1"
        }
      }
    }
  }
}
