{
  "name": "finish",
  "description": "[Git flow]: Finish and merge branch",
  "validate" : {
    "excludes" : ["feature", "hotfix", "release"],
    "type" : "flow"
  },
  "steps": {
    "merge": {
      "implementation-check" : false
    },
    "setVersion": {
      "excludes" : ["feature", "hotfix", "release"]
    },
    "finish": {
      "implementation-check" : false
    },
    "publish": {
      "type" : "flow",
      "params" : {
        "tag": true
      },
      "inputs" : {
        "deletedBranch" : {"finish": "deletedBranch"}
      }
    }
  }
}
